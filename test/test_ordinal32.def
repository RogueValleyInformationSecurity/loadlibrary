; DEF file for test_ordinal32.dll
; This forces imports from OLEAUT32 to be by ordinal rather than by name

LIBRARY test_ordinal32

IMPORTS
    ; OLEAUT32 ordinal imports with stdcall name mangling
    ; Format: internal_name = dllname.ordinal
    SysAllocString@4 = OLEAUT32.2
    SysFreeString@4 = OLEAUT32.6
    SysStringLen@4 = OLEAUT32.7
